{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es2017",
      "DOM",
      "DOM.Iterable"
    ],
    "outDir": "./dist/js",
    "paths": {
      "*": [
        "./src/*"
      ]
    },
    "moduleResolution": "node",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./src/**/*",
  ]
}
